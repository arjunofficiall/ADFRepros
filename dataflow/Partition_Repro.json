{
	"name": "Partition_Repro",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "AzureSqlDatabase1",
						"type": "LinkedServiceReference"
					},
					"name": "SQL",
					"description": "Import data from BrandBlobStorage"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTablesink",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     store: 'sqlserver',",
				"     schemaName: 'sys',",
				"     tableName: 'all_columns',",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     partitionBy('roundRobin', 4)) ~> SQL",
				"SQL sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     outputAssertFailedRows: true) ~> sink1"
			]
		}
	}
}