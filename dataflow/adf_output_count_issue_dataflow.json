{
	"name": "adf_output_count_issue_dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTablesoruce",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				},
				{
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "externalCall1"
				},
				{
					"name": "externalCall2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TCIN as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionBy('roundRobin', 2),",
				"     fileSystem: 'pipelines',",
				"     folderPath: 'adf_issue/in',",
				"     fileName: 'input.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> source1",
				"source(output(",
				"          CONSTRAINT_CATALOG as string,",
				"          CONSTRAINT_SCHEMA as string,",
				"          CONSTRAINT_NAME as string,",
				"          CHECK_CLAUSE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     partitionBy('hash', 1),",
				"     fileSystem: 'pipelines',",
				"     folderPath: 'adf_issue/in',",
				"     fileName: 'input.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> source2",
				"source1 call(output(",
				"          headers as [string,string],",
				"          status as string",
				"     ),",
				"     allowSchemaDrift: true) ~> externalCall1",
				"source2 call(allowSchemaDrift: true) ~> externalCall2",
				"externalCall1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     truncate: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'delimited',",
				"     fileSystem: 'pipelines',",
				"     folderPath: 'adf_issue/out/multiple_partitions',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> sink1",
				"externalCall2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'delimited',",
				"     fileSystem: 'pipelines',",
				"     folderPath: 'adf_issue/out/one_partition',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> sink2"
			]
		}
	}
}