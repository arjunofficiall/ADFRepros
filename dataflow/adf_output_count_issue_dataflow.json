{
	"name": "adf_output_count_issue_dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "AzureSqlDatabase2",
						"type": "LinkedServiceReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "AzureSqlDBsink",
						"type": "LinkedServiceReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     store: 'sqlserver',",
				"     schemaName: 'INFORMATION_SCHEMA',",
				"     tableName: 'dm_database_backups',",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     fileSystem: 'pipelines',",
				"     folderPath: 'adf_issue/in',",
				"     fileName: 'input.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> source2",
				"source2 select(mapColumn(",
				"          DOMAIN_CATALOG,",
				"          DOMAIN_SCHEMA,",
				"          DOMAIN_NAME,",
				"          DATA_TYPE,",
				"          CHARACTER_MAXIMUM_LENGTH,",
				"          CHARACTER_OCTET_LENGTH,",
				"          COLLATION_CATALOG,",
				"          COLLATION_SCHEMA,",
				"          COLLATION_NAME,",
				"          CHARACTER_SET_CATALOG,",
				"          CHARACTER_SET_SCHEMA,",
				"          CHARACTER_SET_NAME,",
				"          NUMERIC_PRECISION,",
				"          NUMERIC_PRECISION_RADIX,",
				"          NUMERIC_SCALE,",
				"          DATETIME_PRECISION,",
				"          DOMAIN_DEFAULT",
				"     ),",
				"     partitionBy('roundRobin', 2),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     store: 'sqlserver',",
				"     schemaName: 'INFORMATION_SCHEMA',",
				"     tableName: 'DOMAINS',",
				"     insertable: true,",
				"     updateable: false,",
				"     deletable: false,",
				"     upsertable: false,",
				"     dateFormat:'MM/dd/yyyy',",
				"     timestampFormat:'MM/dd/yyyy',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          DOMAIN_CATALOG,",
				"          DOMAIN_SCHEMA,",
				"          DOMAIN_NAME,",
				"          DATA_TYPE,",
				"          CHARACTER_MAXIMUM_LENGTH,",
				"          CHARACTER_OCTET_LENGTH,",
				"          COLLATION_CATALOG,",
				"          COLLATION_SCHEMA,",
				"          COLLATION_NAME,",
				"          CHARACTER_SET_CATALOG,",
				"          CHARACTER_SET_SCHEMA,",
				"          CHARACTER_SET_NAME,",
				"          NUMERIC_PRECISION,",
				"          NUMERIC_PRECISION_RADIX,",
				"          NUMERIC_SCALE,",
				"          DATETIME_PRECISION,",
				"          DOMAIN_DEFAULT",
				"     ),",
				"     fileSystem: 'pipelines',",
				"     folderPath: 'adf_issue/out/one_partition',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> sink2"
			]
		}
	}
}